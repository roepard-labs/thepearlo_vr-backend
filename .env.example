# Database configuration
DB_CONNECTION=
DB_HOST=
DB_DATABASE=
DB_PORT=
DB_USERNAME=
DB_PASSWORD=
DB_CHARSET=
# Force secure cookie flag (true/false). Overrides protocol detection.
FORCE_SECURE_COOKIE=
# Custom session name (default ROEPARDSESSID)
SESSION_NAME=
# SameSite policy: Lax | Strict | None
SESSION_SAMESITE=
# Lifetime in seconds (0 = until browser close)
SESSION_LIFETIME=
# Cookie path (default /)
SESSION_COOKIE_PATH=
# Cookie domain (leave empty for current host)
SESSION_COOKIE_DOMAIN=
# Override secure flag explicitly (true/false) if you don't want FORCE_SECURE_COOKIE logic
SESSION_SECURE_OVERRIDE=
# Override HttpOnly (true/false) default true
SESSION_HTTPONLY=
# Enable CSRF protection (true/false)
CSRF_PROTECTION=
# CSRF token lifetime in seconds
CSRF_TOKEN_LIFETIME=
# Allowed origins for CORS (comma separated, * for all)
CORS_ALLOWED_ORIGINS=
# Website URL (used for redirects from API root)
WEBSITE_URL=